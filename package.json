{
  "name": "circuit2",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "remotedev": "remotedev --hostname=localhost --port=8000",
    "lint": "standard --verbose | snazzy",
    "clean": "rm -rf $TMPDIR/react-* && watchman watch-del-all && npm cache clean",
    "newclear": "rm -rf $TMPDIR/react-* && watchman watch-del-all && rm -rf ios/build/ModuleCache/* && rm -rf node_modules/ && npm cache clean && npm i",
    "tron": "node_modules/.bin/reactotron",
    "android:build": "cd android && ./gradlew assembleRelease",
    "android:install": "cd android && ./gradlew assembleRelease && ./gradlew installRelease",
    "android:devices": "$ANDROID_HOME/platform-tools/adb devices",
    "android:logcat": "$ANDROID_HOME/platform-tools/adb logcat *:S ReactNative:V ReactNativeJS:V",
    "codepush": "code-push release-react circuit ios"
  },
  "dependencies": {
    "@shoutem/animation": "^0.8.10",
    "color": "^1.0.3",
    "lottie-react-native": "^1.0.1",
    "mobx": "^2.4.2",
    "mobx-react": "^3.5.4",
    "onecolor": "^3.0.4",
    "react": "^15.4.1",
    "react-native": "^0.40.0",
    "react-native-animatable": "^1.1.0",
    "react-native-autocomplete-input": "^2.0.0",
    "react-native-circular-progress": "0.0.8",
    "react-native-code-push": "^1.17.0-beta",
    "react-native-firestack": "^2.3.9",
    "react-native-modalbox": "^1.3.8",
    "react-native-social-auth": "^0.5.10",
    "react-native-swipe-gestures": "^1.0.1",
    "react-native-textinput-utils": "^0.3.7",
    "react-native-vector-icons": "^4.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native-stage-0": "^1.0.1"
  }
}
